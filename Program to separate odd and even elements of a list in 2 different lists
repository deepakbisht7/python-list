# Program to separate odd and even elements of a list in 2 different lists
def Split(separate): 
    even_list = [] 
    odd_list = [] 
    for i in separate: 
        if (i % 2 == 0): 
            even_list.append(i) 
        else: 
            odd_list.append(i) 
    print("Even lists:", even_list) 
    print("Odd lists:", odd_list) 
  

separate = [1, 2, 3, 4, 5, 6, 7, 8, 9] 
Split(separate)



